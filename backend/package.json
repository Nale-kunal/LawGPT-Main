{
  "name": "lawyer-zen-api",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon -r dotenv/config index.js",
    "start": "node -r dotenv/config index.js",
    "workers": "node src/workers/index.js",
    "test": "node --experimental-vm-modules node_modules/.bin/jest --forceExit",
    "test:coverage": "node --experimental-vm-modules node_modules/.bin/jest --coverage --forceExit",
    "lint": "npx eslint src/ --max-warnings=50",
    "lint:fix": "npx eslint src/ --fix",
    "kill-port": "node -e \"require('child_process').exec('FOR /F \\\"tokens=5\\\" %P IN (\\'netstat -ano ^| findstr :5000 ^| findstr LISTENING\\') DO taskkill /F /PID %P', (err) => { if (err) console.log('Port is free'); else console.log('Port 5000 freed'); })\"",
    "seed:legal": "node src/scripts/seedLegalSections.js",
    "upload-media-to-cloudinary": "node ./scripts/upload-media-to-cloudinary.js",
    "test:db": "node test-api.js"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.6",
    "@sentry/node": "^10.39.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.69.3",
    "cloudinary": "^2.8.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.8.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.10",
    "pino": "^10.3.1",
    "pino-http": "^11.0.0",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.3",
    "rate-limit-redis": "^4.3.1",
    "speakeasy": "^2.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@eslint/js": "^10.0.1",
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "babel-jest": "^30.2.0",
    "eslint": "^10.0.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "pino-pretty": "^13.1.3",
    "supertest": "^7.2.2"
  }
